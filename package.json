{
  "name": "pixa",
  "version": "1.5.2",
  "description": "Get random pic from unsplash.com & pexels.com and push it to Twitter, Telegram & Instagram.",
  "author": "Osama",
  "license": "MIT",
  "repository": "osamaify/pixabot",
  "main": "app.js",
  "private": true,
  "engines": {
    "node": ">= 10.x",
    "pnpm": "4.x"
  },
  "scripts": {
    "start": "node dist/app.js",
    "start:ts": "ts-node src/app.ts",
    "build": "npm run clean && tsc -p .",
    "dev": "nodemon",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^6.0.0",
    "instagram-private-api": "^1.39.2",
    "node-telegram-bot-api": "^0.40.0",
    "twit": "^2.2.11"
  },
  "devDependencies": {
    "@types/imagemin": "^7.0.0",
    "@types/imagemin-jpegtran": "^5.0.0",
    "@types/node": "^13.13.4",
    "@types/node-telegram-bot-api": "^0.40.3",
    "@types/twit": "^2.2.24",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
